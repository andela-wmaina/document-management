    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "superagent": "^3.5.2",
    "validator": "^7.0.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.3.0"
