    "babel-preset-react": "^6.22.0",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.16.1",
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "html-webpack-plugin": "^2.28.0",
    "jsonwebtoken": "^7.3.0",
    "lodash": "^4.17.4",
    "material-ui": "^0.17.1",
    "morgan": "^1.8.1",
    "path": "^0.12.7",
    "pg": "^6.1.2",
    "pg-hstore": "^2.3.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
